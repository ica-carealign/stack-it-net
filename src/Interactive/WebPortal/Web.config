<configuration>
  <connectionStrings>
    <add name="StackIt" connectionString="mongodb://localhost/StackItDev" />
    <add name="HangfireDb" providerName="System.Data.SqlClient" connectionString="Server=(localdb)\MSSQLLocalDB;Integrated Security=true" />
  </connectionStrings>
  <appSettings file="PrivateSettings.config">
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="AWSProfileName" value="default" />
    <add key="AWSRegion" value="us-east-1" />
    <add key="AuthenticationProvider" value="Development" /> <!-- change value="Crowd" to use Crowd authentication -->
    <add key="UnorderedCommandQueue" value=".\Private$\UnorderedCommands" />
    <add key="OrderedCommandQueue" value=".\Private$\OrderedCommands" />
    <add key="MasterKeyAlias" value="example-key-alias" />
    <add key="ConfigurationTemplateBucket" value="example-s3-bucket" />
    <add key="BootstrapperUrl" value="https://bootstrap.example.com/pub/scripts/bootstrap" />
    <add key="PuppetInstallerUrl" value="https://bootstrap.example.com/pub/software/puppet/puppet-3.4.3.msi" />
    <add key="PuppetHost" value="puppetmaster.example.com" /> <!-- must always be the internal ip-based hostname of the puppet master -->
    <add key="PuppetCleanupHost" value="puppetmaster.example.com" /> <!-- *should* be the internal ip based hostname of the puppet master, but may not be for dev -->
    <add key="NodeServicePort" value="8888" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" culture="neutral" publicKeyToken="30ad4fe6b2a6aeed" />
